# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'iOSCodeAssignment' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOSCodeAssignment
	pod 'SwiftLint'
	pod 'RxSwift'
	pod 'RxCocoa'
	pod 'Moya/RxSwift'

  target 'iOSCodeAssignmentTests' do
    inherit! :search_paths
    # Pods for testing
		pod 'RxBlocking'
		pod 'RxTest'

  end

end
